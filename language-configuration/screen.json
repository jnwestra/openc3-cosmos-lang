{
    "comments": {
        "lineComment": "#"
    },
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["<%", "%>"]
    ],
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"],
        ["<%", "%>"]
    ],
    "folding": {
        "markers": {
            "start": "^\\s*(SCREEN|VERTICALBOX|HORIZONTALBOX|TABBOOK|TABITEM|<%|BUTTON\\s+['\"][^'\"]*['\"]\\s+['\"])",
            "end": "^\\s*(END|%>|(?<!\\\\)$)"
        }
    },
    "wordPattern": "(-?\\d*\\.\\d\\w*)|([^\\`\\~\\!\\@\\#\\%\\^\\&\\*\\(\\)\\-\\=\\+\\[\\{\\]\\}\\\\\\|\\;\\:\\'\\\"\\,\\.\\<\\>\\/\\?\\s]+)",
    "indentationRules": {
        "increaseIndentPattern": "^\\s*(SCREEN|VERTICALBOX|HORIZONTALBOX|TABBOOK|TABITEM|<%|BUTTON\\s+['\"][^'\"]*['\"]\\s+['\"].*\\\\\\s*$).*$",
        "decreaseIndentPattern": "^\\s*(END|%>)\\s*$"
    }
}